{
  "name": "bubbles-resume",
  "version": "1.0.0",
  "description": "Interactive gamified resume with D3.js bubble physics and progressive content unlocking",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "server": "node server/index.js",
    "dev:full": "concurrently \"npm run server\" \"npm run dev\"",
    "type-check": "vue-tsc --build --force",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug"
  },
  "dependencies": {
    "better-sqlite3": "^9.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "express": "^4.18.3",
    "gsap": "^3.12.5",
    "helmet": "^7.1.0",
    "joi": "^17.12.2",
    "pinia": "^2.3.1",
    "vue": "^3.5.17",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.1",
    "@types/better-sqlite3": "^7.6.9",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/d3": "^7.4.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.1",
    "@types/vue": "^1.0.31",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/runtime-core": "^3.5.17",
    "@vue/runtime-dom": "^3.5.17",
    "@vue/test-utils": "^2.4.5",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.24.0",
    "happy-dom": "^14.3.9",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.4.3",
    "vite": "^5.2.8",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.7"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "vue3",
    "typescript",
    "d3js",
    "gsap",
    "gamification",
    "resume",
    "portfolio",
    "interactive",
    "bubbles",
    "animation"
  ],
  "author": {
    "name": "Anton Shelekhov",
    "email": "anton.shelekhov@gmail.com",
    "url": "https://antonshelekhov.dev"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/antonshelekhov/bubbles-resume"
  },
  "bugs": {
    "url": "https://github.com/antonshelekhov/bubbles-resume/issues"
  },
  "homepage": "https://antonshelekhov.dev"
}
